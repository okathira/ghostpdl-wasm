{
  "name": "@okathira/ghostpdl-wasm",
  "version": "1.1.0",
  "description": "GhostPDL (Ghostscript) WASM built with Emscripten",
  "keywords": [
    "ghostscript",
    "ghostpdl",
    "wasm",
    "pdf",
    "postscript",
    "emscripten"
  ],
  "license": "AGPL-3.0-or-later",
  "author": "okathira",
  "type": "module",
  "main": "dist/gs.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/gs.js",
      "default": "./dist/gs.js"
    },
    "./gs.js": {
      "types": "./types/index.d.ts",
      "import": "./dist/gs.js",
      "default": "./dist/gs.js"
    },
    "./gs.wasm": "./dist/gs.wasm"
  },
  "files": [
    "dist/",
    "types/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okathira/ghostpdl-wasm"
  },
  "bugs": {
    "url": "https://github.com/okathira/ghostpdl-wasm/issues"
  },
  "homepage": "https://github.com/okathira/ghostpdl-wasm",
  "scripts": {
    "build:docker-ps": "docker run --rm -v ${pwd}:/src emscripten/emsdk:4.0.13 bash -lc './compile.sh'",
    "build:docker-cmd": "docker run --rm -v %cd%:/src emscripten/emsdk:4.0.13 bash -lc './compile.sh'",
    "build": "./compile.sh",
    "optimize:docker-ps": "docker run --rm -v ${pwd}:/src emscripten/emsdk:4.0.13 bash -lc './optimize_with_binaryen.sh'",
    "optimize:docker-cmd": "docker run --rm -v %cd%:/src emscripten/emsdk:4.0.13 bash -lc './optimize_with_binaryen.sh'",
    "optimize": "./optimize_with_binaryen.sh",
    "deploy:dry": "npm publish --dry-run",
    "deploy": "npm publish --access public",
    "deploy:next": "npm publish --access public --tag next"
  },
  "dependencies": {
    "@types/emscripten": "^1.41.5"
  }
}
